%tr{"id" => dom_id(visualization)}
  %th{scope: "row"}= visualization.id
  %td= visualization.label
  %td= visualization.created_at
  %td
    =link_to t('.destroy'), dataset_visualization_path(@dataset, visualization), data: { 'turbo-method' => :delete, confirm: "Are you sure?" }, class: 'btn btn-sm btn-outline-danger'
    =link_to t('.show'), dataset_visualization_path(@dataset, visualization), class: 'btn btn-sm btn-outline-primary'


